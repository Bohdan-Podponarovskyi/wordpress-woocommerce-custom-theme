{
    "name": "avtocase",
    "version": "1.0.0",
    "author": "BP",
    "scripts": {
        "dev": "npm-run-all -p dev:*",
        "dev:esbuild": "dotenvx run node esbuild.config.js",
        "dev:gulp": "dotenvx run gulp",
        "build": "npm-run-all -p build:*",
        "build:esbuild": "dotenvx run --env-file=.env.production node esbuild.config.js",
        "build:gulp": "dotenvx run --env-file=.env.production gulp production",
        "docs": "dotenvx run gulp docs",
        "prettier:check": "prettier --check .",
        "prettier:write": "prettier --write .",
        "svg": "dotenvx run gulp svg",
        "images": "gulp images",
        "fonts": "gulp fonts"
    },
    "dependencies": {
        "gsap": "^3.13.0",
        "gulp": "^5.0.0",
        "gulp-autoprefixer": "^9.0.0",
        "gulp-sass": "^6.0.1",
        "gulp-sourcemaps": "^3.0.0",
        "npm-run-all": "^4.1.5",
        "sass": "^1.58.3",
        "swiper": "^11.2.6",
        "vanilla-lazyload": "^19.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@dotenvx/dotenvx": "^1.44.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.5.0",
        "esbuild": "0.25.4",
        "gulp-jsdoc3": "^3.0.0",
        "gulp-newer": "^1.4.0",
        "gulp-rename": "^2.0.0",
        "gulp-svg-sprite": "^2.0.3",
        "gulp-svgmin": "^4.1.0",
        "prettier": "3.5.3",
        "sassdoc": "^2.7.4",
        "stylelint": "^16.19.1",
        "stylelint-config-standard-scss": "^15.0.0",
        "stylelint-rem-over-px": "^1.0.2",
        "stylelint-scss": "^6.12.0"
    },
    "license": "ISC"
}
